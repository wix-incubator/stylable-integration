    "murmurhash": "^0.0.2",
    "yargs": "^8.0.2"
  },
  "peerDependencies": {
    "stylable": ">=2.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/enhanced-resolve": "^3.0.3",
    "@types/loader-utils": "^1.1.0",
    "@types/memory-fs": "^0.3.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.24",
    "@types/webpack": "^3.0.9",
    "chai": "^4.1.1",
    "concurrently": "^3.5.0",
    "file-loader": "^0.11.2",
