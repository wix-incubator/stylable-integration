    "mocha-loader": "^1.1.1",
    "ncp": "^2.0.0",
    "node-eval": "^1.1.0",
    "postcss": "^6.0.9",
    "stylable": "^3.0.0",
    "ts-mocha": "^1.0.3",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5"
  },
  "files": [
