{
  "name": "stylable-integration",
  "version": "4.1.0",
  "description": "Integration utils for stylable",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "stc": "./dist/src/build.js"
  },
  "scripts": {
    "build": "tsc && ncp test/fixtures/ dist/test/fixtures",
    "webpack": "webpack",
    "pretest": "yarn build",
    "test": "mocha ./dist/test/index.js",
    "prepublish": "npm run build",
    "start": "rimraf dist && yarn build && concurrently --kill-others --success first \"yarn build -- -w\" \"webpack-dev-server --no-info\""
